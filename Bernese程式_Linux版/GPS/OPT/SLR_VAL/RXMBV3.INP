
! Some keywords for automatic output
! ----------------------------------
CAMPAIGN 1  "./DUMMY"
  ## widget = initmenu; pointer = ACTIVE_CAMPAIGN

SESSION_TABLE 1  "${X}/SKL/SESSIONS.SES"
  ## widget = initmenu; pointer = SESSION_TABLE

DESCR_SESSION_TABLE 1  "Session table"

YR4_INFO 1  "1980"
  ## widget = comment
  # $Y+0

SES_INFO 1  "0060"
  ## widget = comment
  # $S+0

USR_INFO 1  "${USER}"
  ## widget = comment

! Environment Variables
! ---------------------
ENVIRONMENT 1  "" "${}"
  ## widget = initmenu; pointer = ENVIR_LIST

! Selection of Rinex Input File(s)
! --------------------------------
OBSFIL 1  "./DUMMY/RAW/$(STA)0060.80M"
  ## widget = selwin; path = DIR_RXM; ext = EXT_RXM; maxfiles = 999
  # $(STA)$S+0

OBSFIL_PTH_COL_2 1  "./DUMMY/ATM/"
  ## widget = initmenu; pointer = DIR_MET

OBSFIL_EXT_COL_2 1  "MET"
  ## widget = initmenu; pointer = EXT_MET

MSG_OBSFIL 1  "RINEX meteo file(s)"

! Station Name Translation Table
! ------------------------------
STAINFO 1  "./DUMMY/STA/$(CRDINF).STA"
  ## widget = selwin; path = DIR_STA; ext = EXT_STA; maxfiles = 1
  ## emptyallowed = true
  # $(CRDINF)

DESCR_STAINFO 1  "Station information"

! Use station information file flags
! ----------------------------------
USEFLG 1  "0"
  ## widget = checkbox; activeif = STAINFO /= _

MSG_USEFLG 1  "Consider flags in sta-info file"

! Flag to use in STAINFO: type 001
! --------------------------------
FLG001 1  "999"
  ## widget = lineedit; activeif = STAINFO /= _ AND USEFLG = 1

MSG_FLG001 1  "Flag to be used"

! Stop if Station Name not found in Translation Table
! ---------------------------------------------------
STNMSTOP 1  "0"
  ## widget = checkbox; activeif = STAINFO /= _

MSG_STNMSTOP 1  "Stop if station name not found"

! NEW/APPEND
! ----------
NEWOLD 1  "NEW"
  ## widget = combobox; editable = false; cards = NEW APPEND

MSG_NEWOLD 1  "NEW/APPEND"

! Bernese Met.Files
! -----------------
METEON 0
  ## widget = lineedit; activeif = NEWOLD = NEW; path = DIR_MET
  ## ext = EXT_MET; maxfiles = 1
  # 

DESCR_METEON 1  "Bernese meteo file (NEW)"

METEOA 0
  ## widget = selwin; activeif = NEWOLD = APPEND; path = DIR_MET
  ## ext = EXT_MET; maxfiles = 1
  # 

DESCR_METEOA 1  "Bernese meteo file (APPEND)"

! Program Output File
! -------------------
SYSODEF 1  "1"
  ## widget = checkbox

MSG_SYSODEF 1  "Program output"

SYSOUT 1  "./DUMMY/OUT/RXMBV3.L00"
  ## widget = lineedit; path = DIR_OUT; ext = EXT_OUT
  ## activeif = SYSODEF == 0; emptyallowed = false
  # RXMBV3

DESCR_SYSOUT 1  "Program output"

! Error Message File
! ------------------
ERRMRG 1  "0"
  ## widget = checkbox

MSG_ERRMRG 1  "Error message"

SYSERR 1  "${U}/WORK/ERROR.MSG"
  ## widget = lineedit; path = PTH_ERR; ext = EXT_ERR
  ## activeif = ERRMRG == 0; emptyallowed = false
  # ERROR

DESCR_SYSERR 1  "Error messages"

! Title Line
! ----------
TITLE 1  "SLRVAR_800060: Convert RINEX to Bernese meteo files: Station $(STA)"
  ## widget = lineedit
  # SLRVAR_$YSS+0: Convert RINEX to Bernese meteo files: Station $(STA)

MSG_TITLE 1  "Title line"

SHOWGEN 1  "1"
  ## widget = checkbox

MSG_SHOWGEN 1  "Show all general files"


