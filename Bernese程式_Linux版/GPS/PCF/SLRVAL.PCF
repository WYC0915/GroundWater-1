# ------------------------------------------
200 QLRNX_1  SLR_VAL           ANY      1 100
#
300 RXMBV_1  SLR_VAL           ANY      1 200
310 RXOBV_1  SLR_VAL           ANY      1 010 200
#
350 CHK_OBS  NO_OPT            ANY      1 300 310
#
# Compute SLR residuals
# ---------------------
400 GPSEST_S SLR_VAL           ANY      1 020 110 350
500 QLRSUM_1 SLR_VAL           ANY      1 400
